{
  "name": "react-generic-table-example",
  "homepage": ".",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p reactStart tailwindcss:watch",
    "build": "run-s reactBuild tailwindcss:build",
    "tailwindcss:build": "npx tailwindcss -i ./src/index.css -o ./build/static/css/index.css",
    "tailwindcss:watch": "npx tailwindcss -i ./src/index.css -o ./build/static/css/index.css --watch",
    "reactStart": "node ../node_modules/react-scripts/bin/react-scripts.js start",
    "reactBuild": "node ../node_modules/react-scripts/bin/react-scripts.js build",
    "reactTest": "node ../node_modules/react-scripts/bin/react-scripts.js test",
    "reactEject": "node ../node_modules/react-scripts/bin/react-scripts.js eject"
  },
  "dependencies": {
    "react": "file:../node_modules/react",
    "react-dom": "file:../node_modules/react-dom",
    "react-generic-table": "file:..",
    "react-scripts": "file:../node_modules/react-scripts",
    "react-live": "^4.1.5",
    "@heroicons/react": "^2.0.18"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.3",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
